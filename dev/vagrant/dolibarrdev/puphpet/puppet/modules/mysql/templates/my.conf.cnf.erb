### MANAGED BY PUPPET ###
<% @settings.sort.each do |section, content|      -%>
[<%= section %>]
<%   content.sort.each do |key, values|          -%>
<%     [values].flatten.sort.each do |value|     -%>
<%= !value ? '#' : '' %><%= key -%><%=
         case value
         when true, false
           ''
         else
           " = #{value}"
         end
%>
<%     end                                       -%>
<%   end                                         -%>

<% end                                           -%>
