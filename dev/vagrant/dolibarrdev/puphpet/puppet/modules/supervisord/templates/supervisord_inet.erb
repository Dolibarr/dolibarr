[inet_http_server]
port=<%= @inet_server_hostname %>:<%= @inet_server_port %>
<% if @inet_auth -%>
username=<%= @inet_username %>
password=<%= @inet_password %>
<% end -%>

[supervisorctl]
serverurl=http://<%= @inet_hostname%>:<%= @inet_server_port %>
<% if @inet_auth -%>
username=<%= @inet_username %>
password=<%= @inet_password %>
<% end -%>
