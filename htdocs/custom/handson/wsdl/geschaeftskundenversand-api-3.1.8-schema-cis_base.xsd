				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="streetNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>House number of the ParcelShop</xs:documentation>
				</xs:annotation>
